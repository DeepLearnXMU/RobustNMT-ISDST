  <form class="js-pick-reaction" data-turbo="false" action="/github/site-policy/reactions" accept-charset="UTF-8" method="post"><input type="hidden" name="_method" value="put" autocomplete="off" /><input type="hidden" name="authenticity_token" value="mqxSJop-9tCoPCd0tJGRuc8PM8GjFddk0f5ch3GFX268dX8guNSWAxXKRAqJpTNlQG8JFnN02OdeuQzGbhIZ7A" autocomplete="off" />
